{
  "manifest_version": 2,
  "name": "JSON Lite",
  "version": "0.13",
  "description": "Fast JSON highlighter - shows items count/size, handles large files, collapse items",
  "browser_action": {
    "default_popup": "README.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
